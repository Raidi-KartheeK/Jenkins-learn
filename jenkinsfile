    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'echo This is Build'
            }
        }
        stage('Test') { 
            steps {
                sh 'This is test'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'This is deploy'
            }
        }
    }
}

